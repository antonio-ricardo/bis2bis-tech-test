    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.3.0",
    "mongoose": "^7.0.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.1",
    "pino": "^8.10.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.21.0",
